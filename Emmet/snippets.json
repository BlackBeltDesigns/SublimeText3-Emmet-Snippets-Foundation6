{
	"variables": {
		"lang": "en",
		"locale": "en-US",
		"charset": "UTF-8",
		"indentation": "\t",
		"newline": "\n"
	},
	
	"html": {
		"filters": "html",
		"profile": "html",
		"snippets": {
			"menu": "ul.menu>li*${1:how_many}>a{Link \\$}",
			"menu:bar": ".top-bar>(.top-bar-left>ul.dropdown.menu[data-dropdown-menu.]>li.menu-text{${1:Site Title}}+li*${2:how_many}>a{\\$}>ul.menu.vertical>li*${4:how_many}>a{\\$})+.top-bar-right>ul.menu>li>inp[type=search][placeholder=\"${6:search}\"]^li>button[type=button].button{${7:Search}}",
			"menu:drop": "ul.dropdown.menu[data-dropdown-menu.]>li*${1:how_many}>a{Item \\$}>ul.menu>li*${2:how_many}>a{Item \\$}",
			"menu:drop:vert": "ul.vertical.dropdown.menu[data-dropdown-menu.]>li*${1:how_many}>a{Item \\$}>ul.menu>li*${2:how_many}>a{Item \\$}",
			"menu:drill": "ul.vertical.menu[data-drilldown.]>li*${1:how_many}>a[href=\"#Item-\\$\"]{Item \\$}>ul.vertical.menu>li*${2:how_many}>a[href=\"#Item-\\$\\$\"]{Item \\$\\$}",
			"menu:accordian": "ul.vertical.menu[data-accordion-menu.]>li*${1:how_many}>a{Item \\$}>ul.menu.vertical.nested>li*${2:how_many}>a{Item \\$\\$}",
			"button:tiny": "a.tiny.button{${1:Tiny Button}}",
			"button:small": "a.small.button{${1:Small Button}}",
			"button:large": "a.large.button{${1:Large Button}}",
			"button:expanded": "a.expanded.button{${1:Expanded Button}}",
			"button:expanded:small": "a.small.expanded.button{${1:Small Expanded Button}}",
			"button:group": ".button-group>a.button{\\$}*${1:how_many}",
			"button:group:small": ".small.button-group>a.button{\\$}*${1:how_many}",
			"button:group:expanded": ".expanded.button-group>a.button{\\$}*${1:how_many}",
			"button:group:stacked": ".stacked.button-group>a.button{\\$}*${1:how_many}",
			"button:group:stacked:small": ".stacked-for-small.button-group>a.button{\\$}*${1:how_many}",
			"button:group:stacked:medium": ".stacked-for-medium.button-group>a.button{\\$}*${1:how_many}",
			"slider": ".slider[data-slider. data-initial-start=${1:start} data-end=${2:end}]>span.slider-handle[data-slider-handle. role=slider tabindex=1]+span.slider-fill[data-slider-fill.]+input:hidden",
			"slider:vert": ".slider.vertical[data-slider. data-initial-start=${1:start} data-end=${2:end} data-vertical=true]>span.slider-handle[data-slider-handle. role=slider tabindex=1]+span.slider-fill[data-slider-fill.]+input:hidden",
			"switch": ".switch>input:checkbox.switch-input#${1:switch_id}[name=${2:switch_name}]+label.switch-paddle[for=${1:switch_id}]>span.show-for-sr{${3:Screen_Reader_Copy}}",
			"switch:tiny": ".switch.tiny>input:checkbox.switch-input#${1:switch_id}[name=${2:switch_name}]+label.switch-paddle[for=${1:switch_id}]>span.show-for-sr{${3:Screen_Reader_Copy}}",
			"switch:small": ".switch.small>input:checkbox.switch-input#${1:switch_id}[name=${2:switch_name}]+label.switch-paddle[for=${1:switch_id}]>span.show-for-sr{${3:Screen_Reader_Copy}}",
			"switch:large": ".switch.large>input:checkbox.switch-input#${1:switch_id}[name=${2:switch_name}]+label.switch-paddle[for=${1:switch_id}]>span.show-for-sr{${3:Screen_Reader_Copy}}",
			"switch:text": ".switch.large>input:checkbox.switch-input#${1:switch_id}[name=${2:switch_name}]+label.switch-paddle[for=${1:switch_id}]>span.show-for-sr{${3:Screen_Reader_Copy}}+span.switch-active[aria-hidden=true]{${4:Active_Text}}+span.switch-inactive[aria-hidden=true]{${5:Inactive_Text}}",
			"switch:radio": ".switch*${1:how_many}>input:radio.switch-input#${2:switch_id}\\$[checked. name=${3:switch_name}]+label.switch-paddle[for=${2:switch_id}\\$]>span.show-for-sr{${4:Screen_Reader_Text}}",
			"pagination": "ul.pagination[role=navigation aria-label=Pagination]>li.pagination-previous[disabled.]{Previous }>span.show-for-sr{page}^li.current>span.show-for-sr{You're on page}+{1}^li*${1:how_many}>a[aria-label=\"Page \\$@2\"]{\\$@2}^li.pagination-next>a[aria-label=\"Next page\"]{Next }>span.show-for-sr{page}",
			"breadcrumbs": "nav[aria-label=\"You are here:\" role=navigation]>ul.breadcrumbs>li>a{Home}^li>a{Page 1}^li.disabled{Disabled}+li>span.show-for-sr{Current: } {Current Page}",
			"accordion": "ul.accordion[data-accordion.]>(li.accordion-item[data-accordion-item.]>a.accordion-title{Accordion}$+.accordion-content[data-tab-content.]{lorem${1:25}})*${2:how_many}",
			"dropdown":	"button.button[type=button data-toggle=${1:element_id}]{${2:Button_Text}}+.dropdown-pane#${1:element_id}[data-dropdown. data-auto-focus=true]{PLACE YOUR CONTENT IN HERE}",
			"dropdown:hover": "button.button[type=button data-toggle=${1:element_id}]{${2:Button_Text}}+.dropdown-pane#${1:element_id}[data-dropdown. data-hover=true data-hover-pane=true]{PLACE YOUR CONTENT IN HERE}",
			"off-canvas": ".off-canvas-wrapper>.off-canvas-wrapper-inner[data-off-canvas-wrapper.]>.off-canvas.position-${1:left-or-right}#offCanvas[data-off-canvas.]>button.close-button[type=button data-close.]>span[aria-hidden=true]{&times;}^ul.vertical.menu>li*4>a{Link-\\$}^^^.off-canvas-content[data-off-canvas-content.]>button.button[type=button data-toggle=offCanvas]{Open Menu}",
			"modal": ".reveal#${1:modal_id}[data-reveal.]>h1{MODAL CONTENT}+p{lorem45}+button.close-button[data-close. type=button]>span[aria-hidden=true]>{&times;}^^^p>button[data-open=${1:modal_id}]{Click me for a modal}",
			"modal:tiny": ".reveal.tiny#${1:modal_id}[data-reveal.]>h1{MODAL CONTENT}+p{lorem45}+button.close-button[data-close. type=button]>span[aria-hidden=true]>{&times;}^^^p>button[data-open=${1:modal_id}]{Click me for a modal}",
			"modal:small": ".reveal.small#${1:modal_id}[data-reveal.]>h1{MODAL CONTENT}+p{lorem45}+button.close-button[data-close. type=button]>span[aria-hidden=true]>{&times;}^^^p>button[data-open=${1:modal_id}]{Click me for a modal}",	
			"modal:large": ".reveal.large#${1:modal_id}[data-reveal.]>h1{MODAL CONTENT}+p{lorem45}+button.close-button[data-close. type=button]>span[aria-hidden=true]>{&times;}^^^p>button[data-open=${1:modal_id}]{Click me for a modal}",	
			"modal:full": ".reveal.full#${1:modal_id}[data-reveal.]>h1{MODAL CONTENT}+p{lorem45}+button.close-button[data-close. type=button]>span[aria-hidden=true]>{&times;}^^^p>button[data-open=${1:modal_id}]{Click me for a modal}",	
			"tables": "table>thead>tr>th{Header Cell \\$}*${1:how_many_columns}^^tbody>tr*${2:how_many_rows}>td{Data Cell \\$}*${1:how_many_columns}^^tfoot>tr>th{Footer Cell \\$}*${1:how_many_columns}",
			"tables:hover": "table.hover>thead>tr>th{Header Cell \\$}*${1:how_many_columns}^^tbody>tr*${2:how_many_rows}>td{Data Cell \\$}*${1:how_many_columns}^^tfoot>tr>th{Footer Cell \\$}*${1:how_many_columns}",
			"tables:stack": "table.stack>thead>tr>th{Header Cell \\$}*${1:how_many_columns}^^tbody>tr*${2:how_many_rows}>td{Data Cell \\$}*${1:how_many_columns}^^tfoot>tr>th{Footer Cell \\$}*${1:how_many_columns}",
			"tables:scroll": ".table-scroll>table>thead>tr>th{Header Cell \\$}*${1:how_many_columns}^^tbody>tr*${2:how_many_rows}>td{Data Cell \\$}*${1:how_many_columns}^^tfoot>tr>th{Footer Cell \\$}*${1:how_many_columns}",
			"tabs": "ul.tabs#${1:tabs_title}[data-tabs.]>li.tabs-title.is-active>a[href=#panel1 aria-selected=true]{Tab 1}^li.tabs-title*${2:how_many_more}>a[href=#panel\\$@2 aria-selected=true]{Tab \\$@2}^^.tabs-content[data-tabs-content=${1:tabs_title}]>.tabs-panel.is-active#panel1>p{lorem52}^.tabs-panel#panel\\$@2*${2:how_many_more}>p{lorem52}",
			"tabs:vert": ".row.collapse>.medium-3.columns>ul.tabs.vertical#${1:tabs_title}[data-tabs.]>li.tabs-title.is-active>a[href=#panel1 aria-selected=true]{Tab 1}^li.tabs-title*${2:how_many_more}>a[href=#panel\\$@2 aria-selected=true]{Tab \\$@2}^^^.medium-9.columns>.tabs-content.vertical[data-tabs-content=${1:tabs_title}]>.tabs-panel.is-active#panel1>p{lorem52}^.tabs-panel#panel\\$@2*${2:how_many_more}>p{lorem52}",
			"video": ".flex-video.widescreen>iframe[width=${1:420} height=${2:315} src=${3:video_url} frameborder=0 allowfullscreen]",
			"video:youtube": ".flex-video.widescreen>iframe[width=${1:420} height=${2:315} src=https://www.youtube.com/embed/${3:video_code} frameborder=0 allowfullscreen]",
			"video:vimeo": ".flex-video.widescreen.vimeo>iframe[width=${1:420} height=${2:315} src=https://player.vimeo.com/video/${3:video_code} frameborder=0 allowfullscreen]",
			"slider:image": ".orbit[role=region aria-label=\"${1:slider-title}\" data-orbit.]>ul.orbit-container>button.orbit-previous>span.show-for-sr{Previous Slide}+{&#9664;&#xFE0E;}^button.orbit-next>span.show-for-sr{Next Slide}+{&#9654;&#xFE0E;}^li.is-active.orbit-slide>img.orbit-image[src=\"${2:image-source}\" alt=\"${3:image-alt}\"]+figcaption.orbit-caption{${4:slide-caption}}^li.orbit-slide*${5:how_many_more}>img.orbit-image[src=\"http://lorempixel.com/400/200\" alt=\"Alt Text\"]+figcaption.orbit-caption{CAPTION TEXT}^^nav.orbit-bullets>button.is-active[data-slide=0]>span.show-for-sr{${4:slide-caption}}+span.show-for-sr{Current Slide}^button[data-slide=\\$]*${5:how_many_more}>span.show-for-sr{#\\$ slide details.}",
			"slider:text": ".orbit[role=region aria-label=\"${1:slider-title}\" data-orbit.]>ul.orbit-container>button.orbit-previous>span.show-for-sr{Previous Slide}+{&#9664;&#xFE0E;}^button.orbit-next>span.show-for-sr{Next Slide}+{&#9654;&#xFE0E;}^li.is-active.orbit-slide>div>p{SLIDE COPY HERE}^^li.orbit-slide*${2:how_many_more}>div>p{SLIDE-\\$@2 COPY HERE}^^^nav.orbit-bullets>button.is-active[data-slide=0]>span.show-for-sr{SLIDE CAPTION}+span.show-for-sr{Current Slide}^button[data-slide=\\$]*${2:how_many_more}>span.show-for-sr{#\\$@2 SLIDE CAPTION.}",
			"tooltip": "span.has-tip.${1:location-top-bottom-left-right}[data-tooltip. aria-haspopup=\"true\" data-disable-hover=\"${2:false}\" tabindex=\"1\" title=\"${3:ToolTip-Inner-Text}\"]{${4:ToolTip-Word(s)}}",
			"equalize": ".row[data-equalizer=\"${1:keyword}\"]>.medium-4.columns[data-equalizer-watch=\"${1:keyword}\"]*3>{CONTENT HERE}"
		},
		
		"abbreviations": {
		}
	}
}
